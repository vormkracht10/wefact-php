{
    "name": "vormkracht10/wefact-php",
    "description": "PHP package to provide a fluent interface to communicate with WeFact",
    "keywords": [
        "vormkracht10",
        "wefact-php"
    ],
    "homepage": "https://github.com/vormkracht10/wefact-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Bas van Dinther",
            "email": "bas@vormkracht10.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.7"
    },
    "require-dev": {
        "laravel/pint": "^1.2",
        "pestphp/pest": "^1.20",
        "phpstan/phpstan": "^1.10",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Vormkracht10\\WeFact\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vormkracht10\\WeFact\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
